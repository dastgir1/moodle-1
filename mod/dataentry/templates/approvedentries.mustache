<nav aria-label="breadcrumb" class="mb-5" id="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a class="text-decoration-none" href="/mod/dataentry/view.php?id={{id}}">{{#str}}entrypage,dataentry{{/str}}</a></li>
    <li class="breadcrumb-item active" aria-current="page">{{#str}}approvedentries,dataentry{{/str}} </li>
  </ol>
</nav>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                 <h3 class="text-center">{{#str}}approvedentries,dataentry{{/str}} </h3>
                 <input type="search" id="search" placeholder="Search Here..." class="border rounded p-2"/>
                 <a href="/mod/dataentry/export_csv.php?id={{id}}"><button type="submit" class="btn btn-primary rounded  " style="float:right;">Export to csv </button></a>
                </div>
                <div class="card-body">
                    <table class="table table-strapped" >
                     <tr>
                        <th>{{#str}}userid,dataentry{{/str}}</th>
                        <th>{{#str}}username,dataentry{{/str}}</th>
                      
                        <th>{{#str}}totaltime,dataentry{{/str}}</th>
                    </tr>
                    <tbody id="datatable">
                        {{#approvedata}}
                            <tr>
                                <td>{{userid}}</td>
                                <td>{{username}}</td>
                              
                                <td>{{totaltime}}</td>
                            </tr>
                        {{/approvedata}}
                    </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>